= form_for :assignment do |f|
	%table
		%tbody
			%tr
				%th Name
				%th Due Date
				%th Type
				%th Percent Done
			- Assignment.all.each do |a|
				%tr
					%td 
						=a.name
					%td 
						=a.due_date.strftime("%m/%d/%Y")
					%td 
						=a.assn_type
					%td 
						=a.percent_done
	= link_to 'New Assignment', new_assignment_path
